@article{carrascal-manzanares_fast_2018,
	title = {A {Fast} {Implementation} of a {Spectral} {Finite} {Elements} {Method} on {CPU} and {GPU} {Applied} to {Ultrasound} {Propagation}},
	copyright = {©2018 \&copy; The authors and IOS Press. All rights reserved.},
	issn = {0927-5452},
	url = {http://www.medra.org/servlet/aliasResolver?alias=iospressISBN&isbn=978-1-61499-842-6&spage=339&doi=10.3233/978-1-61499-843-3-339},
	doi = {10.3233/978-1-61499-843-3-339},
	abstract = {In this paper we present an optimization of a spectral finite element method implementation. The improvements consisted in the modification of the memory layout of the main algorithmic kernels and in the augmentation of the arithmetic intensity via loop transformations. The code has been deployed on multi-core SIMD machines and GPU. Compared to our starting point, i.e. the original scalar sequential code, we achieved a speed up of \&times;228 on CPU. We present comparisons with the SPECFEM2D code that prove the good performances of our implementation on similar cases. On GPU, a hybrid solution is investigated.},
	urldate = {2018-04-09},
	journal = {Advances in Parallel Computing},
	author = {Carrascal-Manzanares, C. and Imperiale, A. and Rougeron, G. and Bergeaud, V. and Lacassagne, L.},
	year = {2018},
	pages = {339--348}
}

